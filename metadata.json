{
    "name": "ncicloud-mariadb",
    "version": "1.2.2",
    "author": "NCI Cloud Team",
    "summary": "Installs, configures and manages Mariadb servers",
    "description": "Installs, configures and manages Mariadb servers",
    "license": "Apache-2.0",
    "source": "https://github.com/NCI-Cloud/puppet-mariadb",
    "issues_url": "https://github.com/NCI-Cloud/puppet-mariadb/issues",
    "dependencies": [
        {
            "name": "puppetlabs/xinetd",
            "version_requirement": ">=1.5.0 <=3.3.0"
        },
        {
            "name": "puppetlabs/mysql",
            "version_requirement": ">=8.0.0"
        },
        {
            "name": "puppetlabs/stdlib",
            "version_requirement": ">=4.0.0"
